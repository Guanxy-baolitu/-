.checkout_list {
  padding: 10px 15px 2px 15px;
  background: #fff;
  border-bottom: 1rpx solid #eee;
}

.checkout_list .flow-shopList {
  padding: 5rpx 0 10rpx;
  border-bottom: 1rpx solid #eee;
}

.checkout_list .flow-shopList:last-child {
  border-bottom: 0;
}
.address-header .flow-checkout-admin {
  font-size: 34rpx;
  color: #444;
}

.address-header .flow-checkout-admin text {
  padding: 0 10rpx;
}

.address-header .flow-checkout-address {
  font-size: 26rpx;
  color: #777;
  margin-top: 6rpx;
}

.order-navbar {
  margin: auto;
  padding: 38rpx 0;
  width: 100%;
  box-shadow: 0 1rpx 5rpx 0px rgba(0, 0, 0, 0.05);
  border-top: 1rpx solid #eee;
  border-radius: 5rpx;
  background: #fff url('data:image/png;base64,')bottom left repeat-x;
  padding-bottom: 48rpx;
}

.clean-order-navbar {
  margin: auto;
  padding: 38rpx 0;
  line-height: 100rpx;
  font-weight: 700;
  font-size: 48rpx;
  width: 100%;
  background-color: #fff;
}

.order-navbar-header {
  margin: auto;
  width: 94%;
  height: 70rpx;
  justify-content: space-between;
  align-items: center;
}
.pinkButton{
  background-color: #EE1376;
  color: #fff;
  border-radius: 60rpx;
  padding-left: 80rpx;
  padding-right: 80rpx;
  padding-top: 0rpx;
  padding-bottom: 0rpx;
}
/** 搜索热点标题 */
.wxSearchTitle{
  display: block;
  padding: 10rpx 5rpx 5rpx 10rpx;
  text-align: left;
  font-weight: 700;
}
/** 标签样式 */
.wxSearchKeyList{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  border-bottom: 1rpx solid #eee;
}
/** 标签样式 */
.wxSearchKeyItem{
  margin: 8rpx;
  padding: 12rpx 28rpx 12rpx 28rpx;
  border-radius: 48rpx;
  border: 1.5rpx solid #eee;
}
.wxSearchKeyItem-select{
  margin: 8rpx;
  padding: 12rpx 28rpx 12rpx 28rpx;
  border-radius: 48rpx;
  color: #fff;
  background: #EE1376;
}