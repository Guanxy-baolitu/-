/**index.wxss**/

page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.swiper-item{
  height: 1000rpx;
}
.page-section_center{
  display: flex;
  flex-direction: column;
}
.page-section:last-child{
  margin-bottom: 0;
}
.page-section-spacing{
  box-sizing: border-box;
  padding-top: 40rpx;
  padding-bottom: 0rpx;
}
.order-navbar {
  margin: auto;
  padding: 38rpx 0;
  line-height: 100rpx;
  font-weight: 700;
  font-size: 50rpx;
  width: 100%;
}

.order-navbar-header {
  margin: auto;
  height: 70rpx;
  justify-content: space-between;
  align-items: center;
}

.pinkButton{
  background-color: #FFC801;
  color: #333333;
  border-radius: 60rpx;
  padding-left: 80rpx;
  padding-right: 80rpx;
  padding-top: 0rpx;
  padding-bottom: 0rpx;
}